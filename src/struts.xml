<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

<package name="default" extends="struts-default">

<action name="register" class="com.javatpoint.RegisterAction">
<result name="success">WinnerAcknowledgement.jsp</result>
<result name="error">SimpleAcknowledgement.jsp</result>
</action>

<action name="retrieve" class="com.javatpoint.RetrieveAction">
<result name="success">Student.jsp</result>
<result name="error">NoStudent.jsp</result>
</action>

</package>
</struts>    
